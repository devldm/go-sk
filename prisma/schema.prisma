generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model go_sk_jobs {
  job_id          String  @id @db.Uuid
  job_title       String
  company_name    String
  location        String
  job_description String
  linkedin_url    String?
  apply_url       String?

  @@map("go-sk-jobs")
}
